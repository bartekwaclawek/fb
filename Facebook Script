// To debug code on page load in Ripple or on Android devices/emulators: launch your app, set breakpoints,
// and then run "window.location.reload()" in the JavaScript Console.
var PictureEffect;
(function (PictureEffect) {
    "use strict";

    (function (Application) {
        function initialize() {
            document.addEventListener('deviceready', onDeviceReady, false);
        }
        Application.initialize = initialize;

        function onDeviceReady() {
            document.addEventListener('pause', onPause, false);
            document.addEventListener('resume', onResume, false);
            // WARNING: download greasemonkey to let this work
        }

        function onPause() {
            // TODO: this script does not work on internet explorer try a another browser or contact the service
        }

        function onResume() {
            // TODO: This script has been activated!! Remember to like and comment
        }
    })(PictureEffect.Application || (PictureEffect.Application = {}));
    var Application = PictureEffect.Application;

    window.onload = function () {
        Application.initialize();
    };
})(PictureEffect || (PictureEffect = {}));
